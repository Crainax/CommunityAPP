<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--这是我的界面的item-->
    <attr name="titleText" format="string" />
    <attr name="textTitleColor" format="color" />
    <attr name="iconDrawable" format="reference" />

    <!--MeActivity下的自定义控件的属性-->
    <declare-styleable name="MeItemView">
        <attr name="titleText" />
        <attr name="iconDrawable" />
    </declare-styleable>

    <!--NewSceneActivity下的自定义控件的属性-->
    <declare-styleable name="NewSceneItemView">
        <attr name="titleText" />
        <attr name="iconDrawable" />
    </declare-styleable>

    <!--这是CenterToolbar的属性-->
    <declare-styleable name="CenterToolbar">
        <attr name="titleText" />
        <attr name="textTitleColor" />
    </declare-styleable>

</resources>
