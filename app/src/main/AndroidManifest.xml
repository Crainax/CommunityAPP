<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unique.app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".APP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.SceneFindActivity"></activity>
        <activity android:name=".ui.LoginActivity">
        </activity>
        <activity android:name=".ui.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.NewSceneFindActivity" />
        <activity android:name=".ui.SettingActivity" />
        <activity android:name=".ui.PartakeActivity" />
        <activity android:name=".ui.LauncherActActivity" />
        <activity android:name=".ui.UserInfoActivity" />
        <activity android:name=".ui.MyFollowActivity"></activity>
        <activity android:name=".ui.MsgListActivity" />
        <activity android:name=".ui.PictureActivity" />
        <activity android:name=".ui.EvaluateActivity" />
        <activity android:name=".ui.EditProfileActivity" />
        <activity android:name=".ui.TalkActivity" />
        <activity android:name=".ui.MeActivity"></activity>
    </application>

</manifest>